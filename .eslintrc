{
  "root": true,
  "extends": ["plugin:@shopify/typescript", "plugin:@shopify/prettier"],
  "rules": {
    "import/order": [0]
  },
  "overrides": [
    {
      "files": ["src/**/*.module.ts"],
      "rules": {
        "@typescript-eslint/no-extraneous-class": [0]
      }
    },
    {
      "files": ["src/**/*.config.ts"],
      "rules": {
        "no-process-env": [0]
      }
    }
  ]
}
